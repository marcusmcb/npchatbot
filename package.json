{
  "name": "serato-nowplaying-twitch",
  "version": "1.0.0",
  "description": "!np (now playing) Twitch Chat Bot and command built with TMI.js",
  "main": "electron-main.js",
  "scripts": {
    "start": "BROWSER=none react-scripts start",
    "start-client": "npm start --prefix client",
    "start-electron": "electron .",
    "dev": "concurrently --kill-others \"npm run start-client\" \"wait-on http://127.0.0.1:3000 && npm run start-electron\""
  },
  "keywords": [
    "twitch",
    "serato",
    "node",
    "bot",
    "javascript",
    "streaming-tools"
  ],
  "author": "Marcus McBride",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "cheerio": "^1.0.0-rc.12",
    "concurrently": "^8.2.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "electron-reload": "^2.0.0-alpha.1",
    "express": "^4.18.2",
    "keytar": "^7.9.0",
    "nedb": "^1.8.0",
    "nodemon": "^3.0.1",
    "obs-websocket-js": "^5.0.3",
    "tmi.js": "^1.8.5",
    "wait-on": "^7.2.0",
    "ws": "^8.16.0"
  },
  "engines": {
    "node": "14.x",
    "npm": "9.x"
  },
  "devDependencies": {
    "electron": "^26.1.0",
    "electron-is-dev": "^2.0.0"
  }
}
